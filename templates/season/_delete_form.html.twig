<form
    method="post"
    action="{{ path('season_delete', {'id': season.id}) }}"
    onsubmit="{{ 'seasons.delete_onsubmit'|trans }}"
    >
    <input
        type="hidden"
        name="_token"
        value="{{ csrf_token('delete' ~ season.id) }}"
    >
    <button class="btn btn-danger">
        {{ 'nav.delete'|trans }}
    </button>
</form>
